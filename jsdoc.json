{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": [".", "README.md"],
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "excludePattern": "node_modules"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "./readme.md"
  },
  "sourceType": "module"
}
